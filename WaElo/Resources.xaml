<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:WaElo"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
  <sys:String x:Key="strAppTitle">给八云的 WA Elo 系统</sys:String>
  <sys:String x:Key="strAddUserPrompt">添加玩家</sys:String>
  <local:Model x:Key="Model"></local:Model>
  <local:GlobalVars x:Key="GlobalVars"></local:GlobalVars>
  <Style TargetType="Button">
    <Style.Setters>
      <Setter Property="Margin" 
              Value="10"></Setter>
      <Setter Property="Padding"
              Value="5"></Setter>
    </Style.Setters>
  </Style>
  <Style TargetType="TextBlock">
    <Style.Setters>
      <Setter Property="HorizontalAlignment"
              Value="Center"></Setter>
    </Style.Setters>
  </Style>
</ResourceDictionary>